<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="vboxMain" alignment="TOP_CENTER" prefWidth="330.0" style="-fx-background-color: #454b75;" stylesheets="@../css/button.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.nobility.downloader.utils.ChoiceController">
   <children>
      <Label fx:id="lbTitle" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="3.0" text="Title" textAlignment="CENTER" textFill="WHITE" wrapText="true">
         <font>
            <Font name="Arial Black" size="16.0" />
         </font>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </Label>
      <Label fx:id="lbContent" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="3.0" text="Content" textAlignment="CENTER" textFill="WHITE" wrapText="true">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font>
      </Label>
      <Button fx:id="button1" mnemonicParsing="false" text="Button">
         <VBox.margin>
            <Insets bottom="15.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="button2" mnemonicParsing="false" text="Button">
         <VBox.margin>
            <Insets bottom="15.0" left="10.0" right="10.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="button3" mnemonicParsing="false" text="Button">
         <VBox.margin>
            <Insets bottom="15.0" left="10.0" right="10.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="button4" mnemonicParsing="false" text="Button">
         <VBox.margin>
            <Insets bottom="15.0" left="10.0" right="10.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
